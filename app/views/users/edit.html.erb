<h2>Editing user</h2>

<% form_for(@user) do |f| %>
    <%= f.error_messages %>
    <%= render :partial => "user", :locals => { :editable => true } %>

    <div class="buttonPanel">
        <%= f.submit 'Update' %>
        <%= link_to 'Show', @user %>
        <%= link_to 'Back', users_path %>
    </div>
<% end %>

